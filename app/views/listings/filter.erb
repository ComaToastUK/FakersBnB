  <% @listings.each do |listing| %>
    <% if ((@filter_by_location == listing.location) && (@min_price.to_i <= listing.price.to_i) && (@max_price.to_i >= listing.price.to_i))%>
      <ul id='listings_filtered'>
        <%= listing.title %>
        <%= listing.location %>
        <%= listing.price %>
        <img src="<%=listing.imageurl%>" width="300px" />
      </ul>
    <%end%>
  <%end%>
